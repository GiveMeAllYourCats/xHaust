    "coveralls": "^3.1.0",
    "express": "^4.17.1",
    "husky": "^4.2.5",
    "jsdoc": "^3.6.5",
    "mocha": "^8.1.1",
    "mocha-lcov-reporter": "^1.3.0",
    "mocha-logger": "^1.0.6",
    "moment": "^2.27.0",
    "nyc": "^15.1.0",
    "touch": "^3.1.0"
  }
