{
  "name": "xhaust",
  "version": "0.0.5",
  "description": "Blazingly fast brute forcer made in Node.js, exhausting your logins... For science.",
  "main": "xhaust.js",
  "scripts": {
    "test": "npx nyc mocha tests/mocha/**/*.js --slow 1 && npx nyc report --reporter=html",
    "coverage": "npx nyc check-coverage",
    "coveralls:export": "npx nyc report --reporter=text-lcov | coveralls"
  },
  "author": "GiveMeAllYourCats <specamps@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=13.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/givemeallyourcats/xhaust.git"
  },
  "keywords": [
    "bruteforce",
    "brute",
    "force",
    "opsec",
    "security",
    "cybersecurity"
  ],
  "bugs": {
    "url": "https://github.com/givemeallyourcats/xhaust/issues",
    "email": "specamps@gmail.com"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "nyc": {
    "branches": 5,
    "lines": 5,
    "functions": 5,
    "statements": 5,
    "exclude": [
      "tests/mocha"
    ]
  },
  "bin": "./bin/index.js",
  "preferGlobal": true,
  "dependencies": {
    "app-root-path": "^3.0.0",
    "async": "^3.2.0",
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.8.20",
    "bookshelf": "^1.2.0",
    "chalk": "^4.1.0",
    "chalk-rainbow": "^1.0.0",
    "cli-progress": "^3.8.2",
    "commander": "^6.0.0",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "dotfile": "0.0.2",
    "emittery": "^0.7.1",
    "fast-html-parser": "^1.0.1",
    "figlet": "^1.5.0",
    "get-folder-size": "^2.0.1",
    "glob": "^7.1.6",
    "jsdom": "^16.4.0",
    "json-colorizer": "^2.2.2",
    "knex": "^0.21.5",
    "lodash": "^4.17.20",
    "object-inspect": "^1.8.0",
    "ololog": "^1.1.161",
    "ora": "^5.0.0",
    "pg": "^8.3.2",
    "pretty-error": "^2.1.1",
